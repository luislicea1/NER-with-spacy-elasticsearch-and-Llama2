{
  "name": "paper-dashboard-react",
  "version": "1.3.2",
  "private": true,
  "dependencies": {
    "bootstrap": "4.6.2",
    "chart.js": "3.9.1",
    "perfect-scrollbar": "1.5.5",
    "react": "18.2.0",
    "react-chartjs-2": "3.3.0",
    "react-dom": "18.2.0",
    "react-notification-alert": "0.0.13",
    "react-router-dom": "6.11.1",
    "react-scripts": "5.0.1",
    "reactstrap": "8.10.0",
    "sass": "1.62.1"
  },
  "scripts": {
